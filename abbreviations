abbr g="git"
abbr ga="git add"
abbr gb="git branch"
abbr gc="git commit -v"
abbr gd="git diff"
abbr gl="git log"
abbr gp="git push"
abbr gs="git status"
abbr gco="git checkout"
abbr gcp="git cherry-pick"
abbr glo="git log --oneline -10"
abbr gds="git diff --staged"
abbr gcm="git commit -v -m"
abbr gba="git branch -a"
abbr gwa="echo  | xargs -I@ git worktree add $(dirname $(pwd))/$(git symbolic-ref --short HEAD)/@ @"
abbr gwr="git worktree list | awk '{print \"git worktree remove \" $1}' | fzf | zsh"
abbr gwl="git worktree list"

abbr ls="ls -GFS"
abbr la="ls -aA"
abbr lal="ls -a -lA"

abbr cp="cp -i"
abbr mv="mv -i"
